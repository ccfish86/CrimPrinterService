<Application x:Class="CrimPrintService.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CrimPrintService"
             StartupUri="MainWindow.xaml"
             >
    <Application.Resources>
        <local:InverseBoolConverter x:Key="InverseBoolConverter" />
        <local:BoolRadioConverter x:Key="BoolRadioConverter" />
        <local:BoolRadioConverter x:Key="InverseBoolRadioConverter" Inverse="True" />
        <local:BoolToVisibilityConvert x:Key="BoolToVisibilityConvert" />
        <local:ConnectStateToDisplayConvert x:Key="ConnectStateToDisplayConvert" />
    </Application.Resources>
</Application>
